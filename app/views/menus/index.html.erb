
<div id="page-content np-red" class="span-24 np-red np-padding last">
<h1>Listing menus</h1>

<table>
  <tr>
    <th>Startdate</th>
    <th>Enddate</th>
    <th>Pickupdate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @menus.each do |menu| %>
  <tr>
    <td><%= menu.startDate %></td>
    <td><%= menu.endDate %></td>
    <td><%= menu.pickUpDate %></td>
    <td><%= link_to 'Show', menu %></td>
    <td><%= link_to 'Edit', edit_menu_path(menu) %></td>
    <td><%= link_to 'Destroy', menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Menu', new_menu_path %>
</div>
